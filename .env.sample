# AgentXthics Environment Variables

# LLM API Keys (uncomment and set the one you want to use)
GEMINI_API_KEY=your-gemini-api-key-here
# OPENAI_API_KEY=your-openai-api-key-here
OPENAI_API_KEY=api-key
# Optional: Configure ethical framework weights (0.0 to 1.0)
UTILITARIAN_WEIGHT=0.7
DEONTOLOGICAL_WEIGHT=0.5
VIRTUE_ETHICS_WEIGHT=0.6
CARE_ETHICS_WEIGHT=0.8
JUSTICE_WEIGHT=0.7

# Optional: Simulation parameters
ENABLE_ETHICAL_FRAMEWORKS=true
ENABLE_ASYMMETRIC_INFORMATION=false
ENABLE_VULNERABLE_POPULATIONS=false

# Rename this file to .env to use these settings
